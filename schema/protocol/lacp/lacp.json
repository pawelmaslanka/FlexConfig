{
    "properties": {
        "protocol": {
            "properties": {
                "lacp": {
                    "description": "Link Aggregation Control Protocol",
                    "type": "object",
                    "patternProperties": {
                        "^(lag-(6[5-6])|lag-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9]))$": {
                            "type": "object",
                            "properties": {
                                "members": {
                                    "description": "Members of Aggregate Interface",
                                    "type": "object",
                                    "patternProperties": {
                                        "^(eth[0-9]-(6[5-6])|eth[0-9]-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9])(_[1-4])?)$": {
                                            "type": "object",
                                            "properties": {
                                            },
                                            "additionalProperties": false,
                                            "reference": [
                                                "/interface/aggregate-ethernet[@item]/members/@"
                                            ],
                                            "action": {
                                                "on-update-path": "/update",
                                                "on-delete-path": "/delete",
                                                "server-address": "127.0.0.1:8000"
                                            },
                                            "update-dependencies": [
                                                "/interface/aggregate-ethernet[@item]/members"
                                            ],
                                            "update-constraints": [
                                                "must(exists(xpath(@)))"
                                            ]
                                        }
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "additionalProperties": false,
                            "reference": [
                                "/interface/aggregate-ethernet/@"
                            ],
                            "update-dependencies": [
                                "/interface/aggregate-ethernet/@",
                                "/interface/aggregate-ethernet[@item]/protocol"
                            ],
                            "update-constraints": [
                                "must(exists(xpath(@)))",
                                "must(xpath_value('/interface/aggregate-ethernet[@item]/protocol') == 'lacp')"
                            ],
                            "action": {
                                "on-update-path": "/update",
                                "on-delete-path": "/delete",
                                "server-address": "127.0.0.1:8000"
                            }
                        }
                    },
                    "additionalProperties": false
                }
            }
        }
    }
}
