{
    "properties": {
        "protocol": {
            "properties": {
                "rstp": {
                    "description": "Rapid Spanning Tree Protocol",
                    "type": "object",
                    "patternProperties": {
                        "^(br-(6[5-6])|br-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9]))$": {
                            "type": "object",
                            "properties": {
                                "members": {
                                    "description": "Members of the bridge",
                                    "type": "object",
                                    "patternProperties": {
                                        "^(eth[0-9]-(6[5-6])|eth[0-9]-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9])(_[1-4])?)$": {
                                            "type": "object",
                                            "properties": {
                                            },
                                            "additionalProperties": false,
                                            "reference": [
                                                "/interface/ethernet/@"
                                            ],
                                            "update-dependencies": [
                                                "/interface/ethernet[@item]"
                                            ],
                                            "update-constraints": [
                                                "must(exists(xpath(@)))",
                                                "must(exists(xpath_any('/interface/aggregate-ethernet/*/members/@')) == false)"
                                            ],
                                            "action": {
                                                "on-update-path": "/update",
                                                "on-delete-path": "/delete",
                                                "server-address": "127.0.0.1:8000"
                                            }
                                        },
                                        "^(lag-(6[5-6])|lag-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9]))$": {
                                            "type": "object",
                                            "properties": {
                                            },
                                            "additionalProperties": false,
                                            "reference": [
                                                "/interface/aggregate-ethernet/@"
                                            ],
                                            "update-dependencies": [
                                                "/interface/aggregate-ethernet[@item]",
                                                "/interface/aggregate-ethernet[@item]/members"
                                            ],
                                            "update-constraints": [
                                                "must(exists(xpath(@)))"
                                            ],
                                            "action": {
                                                "on-update-path": "/update",
                                                "on-delete-path": "/delete",
                                                "server-address": "127.0.0.1:8000"
                                            }
                                        }
                                    },
                                    "additionalProperties": false
                                }
                            },
                            "additionalProperties": false
                        }
                    },
                    "additionalProperties": false
                }
            }
        }
    }
}
