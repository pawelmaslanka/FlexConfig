interface: group {
    description: "Interface configuration"
    aggregate-ethernet: container {
        description: "Aggregate Ethernet Interfaces collection"
        max: 32
        @item {
            pattern-name: "^ae-(6[5-6])|ae-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9])$"
            members: array {
                max: 32
                @item {
                    ref: "/interface/gigabit-ethernet/@item"
                }
                update-depends: /interface/gigabit-ethernet
            }
        }
    }
    gigabit-ethernet: container {
        description: "Ethernet interfaces collection"
        min: 1
        max: 32
        @item {
            pattern-name: "^ge-(6[5-6])|ge-(6[0-6]|5[0-9]|4[0-9]|3[0-9]|2[0-9]|1[0-9]|[1-9])(\/[1-4])?$"
            speed: leaf {
                type: number
                pattern-value: "(100G|400G)"
                default: 100G
            }
        }
        update-depends: /platform/port
    }
}